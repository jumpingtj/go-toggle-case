{
  "name": "go-toggle-case",
  "displayName": "Go Toggle Case",
  "description": "Toggles whether the name under cursor acts as exported or unexported (capitalized or lowercase)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:go-toggle-case.toggleExported"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "go-toggle-case.toggleExported",
        "title": "Toggle Exported/Unexported"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
